{
  "expo": {
    "name": "Bunksheet",
    "description": "College Life Made Easy!",
    "slug": "bunksheet",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.BunkSheet.BunkSheet",
      "googleServicesFile": "./google-services.json"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}